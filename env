## install location
declare -A FILELOC

FILELOC[baliases]=~/.baliases
FILELOC[bashrc]=~/.bashrc
FILELOC[benv]=~/.benv
FILELOC[benv.priv]=~/.benv.priv
FILELOC[bfunctions]=~/.bfunctions
FILELOC[gdbinit]=~/.gdbinit
FILELOC[vimrc]=~/.vimrc
FILELOC[tmux.conf]=~/.tmux.conf
FILELOC[i3config]=~/.config/i3/config
FILELOC[i3statusconfig]=~/.config/i3status/config
FILELOC[daily_quotes.txt]=~/.daily_quotes.txt

export FILELOC;
